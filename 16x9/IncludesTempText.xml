<!--  Display Temp-Sensor Output  -->

<includes>	
	<include name="TempText1">
		<control type="label">
			<description>Temp Top Left</description>
			<posx>45</posx>
			<posy>10</posy>
			<height>40</height>
			<width>500</width>
			<label>$INFO[Window(Home).Property(Temp1)] $INFO[Window(Home).Property(Text1)]</label> 
			<align>left</align>
			<aligny>center</aligny>
			<font>font40_title</font>
			<textcolor>$VAR[CustomColorNofocus]</textcolor>
		</control> 
	</include>    
