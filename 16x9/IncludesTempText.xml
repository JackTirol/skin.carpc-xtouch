<includes>	
	<include name="TempText1">
		<control type="label">
			<description>Temp Top Left</description>
			<posx>45</posx>
			<posy>10</posy>
			<height>40</height>
			<width>500</width>
			<label>$INFO[Window(Home).Property(Temp1)] 1</label> 
